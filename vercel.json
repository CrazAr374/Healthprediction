{
  "builds": [
    { "src": "app.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "app.py" }
  ]
}
{
  "functions": {
    "api/*.py": {
      "runtime": "python@latest",
      "maxDuration": 10
    }
  },
  "ignoreBuildStep": [
    "static/uploads/",
    "logs/"
  ]
}
